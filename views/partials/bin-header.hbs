<a href="javascript:void(0)" aria-label="Close Widget" onclick="closes(0,5);" class="boxClose1 cwObj " tabindex="1"><img
        alt="Close Widget" src="https://s3.cartwire.co/widget//v2.0/images_retailPopup/close_icon.png"
        class="cw_widget_close closebutton_cw"></a>
<h2 class="cw_product_title" aria-label="{{ title }}">{{ title }}</h2>
<div class="cw_product_wrap">
    <div class="cw_product_img">
        <div class="cw_product_img_wrap " aria-label="{{widget_name}}"><img
                src="{{image_url}}"
                alt="{{widget_name}}" onerror="err()"></div>
    </div>
    <div class="cw_product_content no_scroll_moreless">
        <h3 class="cw_product_sub_title" aria-label="{{widget_name}}"
            style="display:block">{{widget_name}}</h3>
    </div>
</div>
<div class="cwCountryList_wrap"></div>
