<table id="cwRetailTbl" aria-readonly="true" role="grid">
    <tbody class="th4">

        {{#each retailer}}
        {{#if availability}}
        <tr class="price_filter">
            <td aria-label="{{retailer_name}}"><img
                    src="https://s3.cartwire.co/widget/v2.0/images_retailPopup/{{retailer_logo}}" alt="{{retailer_name}}"
                    class="cw_widget_logo"></td>
            <td aria-label="{{currency_symbol}} {{price_retailer_format}} {{stock_txt}}">
                <div class="cw_widget_price_in_stock">{{currency_symbol}}<span class="getPrice " data-price="{{price_retailer_format}}">{{price_retailer_format}}</span> <span
                        class="mpack"></span><span class="cw_widget_stock">{{stock_txt}}</span></div>
                <span class="offer_mobile offer"></span>
            </td>
            <td class="chk offer " aria-label="" alt=""></td>
            <td><a target="_blank"
                    href="https://www.amazon.co.uk/Dove-DermaSpa-Revived-moisturisation-Essentials/dp/B087XLBDHJ/ref=sr_1_66?tag=Dove---UK-uk-bin-button-retailer-click-amazon-21"
                    class="cw_widget_buy_btn cwObj" aria-label="Amazon £5.50 Buy  opens in a new window"
                    onclick="getChildUrlData('https://www.amazon.co.uk/Dove-DermaSpa-Revived-moisturisation-Essentials/dp/B087XLBDHJ/ref=sr_1_66?tag=Dove---UK-uk-bin-button-retailer-click-amazon-21','d7aa2402e8138e016cebe5daf96eaa33','','https%3A%2F%2Fwww.dove.com%2Fuk%2Fp%2Fdermaspa-gradual-self-tan-body-mousse-medium-to-dark.html%2F08710847962783','62e91125426c9be0334eebd7','62e90c6399b1b618f2f04aab','DermaSpa Gradual Self-Tan Body Mousse Medium to Dark 150ml','','5.50','undefined','Amazon_UK','1','£','£','1');"
                    alt="Buy" rel="nofollow" tabindex="3">Buy</a></td>
        </tr>
        {{/if}}
        {{/each}}

    </tbody>
</table>